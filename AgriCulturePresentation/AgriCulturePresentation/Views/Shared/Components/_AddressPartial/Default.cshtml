@using EntityLayer.Concrete
@model List<Address>

<div class="contact-agile" id="contact">
	<div class="container">
		<h3 class="tittle">Bize Ulaşın</h3>
		<p class="heading-btm-w3ls">Aşağıdaki form üzerinden bize mail gönderebilir veya telefon ile arayabilirsiniz.</p>
		<div class="col-md-3 contact-left">
			@foreach (var item in Model)
			{
				<h4>Adresimiz</h4>
				<p>@item.Description1</p>
				<p>@item.Description2</p>
				<p>@item.Description3</p>
				<h4>Telefon Numaramız</h4>
				<p>@item.Description4</p>
			}
			

		</div>
